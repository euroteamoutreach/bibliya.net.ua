{
  "name": "bibliya.net.ua",
  "version": "1.0.0",
  "description": "Ukrainian landing page for Bible First.",
  "author": "Joshua Steele",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint-css": "stylelint '**/*.(css|vue)' --fix",
    "lint-staged": "$(yarn bin)/lint-staged",
    "start": "nuxt start"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.js": [
      "eslint --fix"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/pro-light-svg-icons": "^5.1.0",
    "@fortawesome/pro-regular-svg-icons": "^5.1.0",
    "@fortawesome/pro-solid-svg-icons": "^5.1.0",
    "@fortawesome/vue-fontawesome": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "nuxt": "^1.0.0",
    "vue-scrollto": "^2.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "tailwindcss": "^0.6.1"
  }
}
